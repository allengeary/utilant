<div class="card" style="border: 1px solid black; border-radius: 5px; padding: 10px; margin-top: 25px">
    <div class="card-body">
        <h5 class="card-title">Photos</h5>
        <div class="card-text">
            <div class="row">
                @if (Model != null)
                {
                    @foreach (var p in Model)
                    {
                        <div class="col-md-3">
                            <div class="card">
                                <img class="card-img-top" src="@p.thumbnailUrl" />
                                <div class="card-body">
                                    <h5 class="card-title">@p.title</h5>
                                </div>
                            </div>

                        </div>
                    }

                }
            </div>
        </div>
        </div>
    </div>

    
